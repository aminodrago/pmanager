  FROM pg_constraint ct
  JOIN pg_class cl ON cl.oid=conrelid
  JOIN pg_namespace nl ON nl.oid=cl.relnamespace
  JOIN pg_class cr ON cr.oid=confrelid
  JOIN pg_namespace nr ON nr.oid=cr.relnamespace
